{
    "name": "poe-ladder-bot",
    "description": "Bot to help you find out more information about PoE Ladder",
    "version": "0.0.1",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git://github.com/halfacandan/PoELadderBot.git"
    },
    "main": "dist/bot.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/bot.js",
        "dev": "npm run build && node -r dotenv/config dist/bot.js dotenv_config_path=.env.local",
        "spell-check": "cspell \"*.md\" --config cspell.json"
    },
    "ts-node": {
        "esm": true,
        "experimentalEsm": true
    },
    "dependencies": {
        "@discordjs/builders": "^1.13.1",
        "@discordjs/rest": "^2.6.0",
        "@thatiemsz/text2png": "^2.3.3",
        "base64url": "^3.0.1",
        "discord.js": "^14.25.1",
        "dropin-request": "^1.1.1",
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "cspell": "^9.6.4",
        "dotenv": "^17.2.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
